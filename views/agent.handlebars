<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8" />
	<title>Dashboard</title>

	  <link rel="stylesheet" href="//cdn.jsdelivr.net/bootstrap.material-design/0.3.0/css/material-fullpalette.css" />
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
  <link rel="stylesheet" href="css/main.css" />
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script src="scripts/handlebars-v3.0.3.js"></script>

</head>
<body>
	<nav class="navbar navbar-default navbar-static-top">
	<div class="container-fluid">
		<!-- Brand and toggle get grouped for better mobile display -->
		<div class="navbar-header">
			<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
				<span class="sr-only">Toggle navigation</span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
			</button>
			<a class="navbar-brand" href="#">
				Uber Driver
			</a>
		</div>

		<!-- Collect the nav links, forms, and other content for toggling -->
		<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">			
			<form class="navbar-form navbar-left" method="GET" role="search">
				<div class="form-group">
					<input type="text" name="q" class="form-control" placeholder="Search">
				</div>
				<button type="submit" class="btn btn-default"><i class="glyphicon glyphicon-search"></i></button>
			</form>
			<ul class="nav navbar-nav navbar-right">
				<li><a href="http://www.pingpong-labs.com" target="_blank">Visit Site</a></li>
				<li class="dropdown ">
					<a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">
						Account
						<span class="caret"></span></a>
						<ul class="dropdown-menu" role="menu">
							<li class="dropdown-header">SETTINGS</li>
							<li class=""><a href="#">Pending</a></li>
							<li class=""><a href="#">Issues</a></li>
							<li class=""><a href="#">Drivers</a></li>
							<li class="divider"></li>
							<li><a href="#">Logout</a></li>
						</ul>
					</li>
				</ul>
			</div><!-- /.navbar-collapse -->
		</div><!-- /.container-fluid -->
	</nav>
	<div class="container-fluid main-container">
		<div class="col-md-2 sidebar">
			<ul class="nav nav-pills nav-stacked">
				<li class="active"><a href="#">Home</a></li>
				<li><a href="#" id='drivers-button'>Drivers</a></li>
				<li><a href="#" id='faqs-button'>FAQs</a></li>
				<li><a href="#" id='issues-button'>Issues</a></li>
				
			</ul>
		</div>
		<div class="col-md-10 content" id='mainText'>
			<h1> Agent Dashboard</h1>
            <h4> View and manage drivers and issues</h4>
		</div>





		<footer class="pull-left footer">
			<p class="col-md-12">
				<hr class="divider">
				
			</p>
		</footer>
	</div>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-material-design/0.3.0/js/material.js"></script>

	<script type='text/x-handlebars-template' id ='faqs-html'>
					<h6> Frequently Asked Questions</h6>
					
					<button type="button" class="btn btn-default">Add New </button>
					<br>
					<div class="panel-group" id="accordion">
					 	{{#each Questions}}
				        <div class="panel panel-default">
				            <div class="panel-heading">
				                <h4 class="panel-title">
				                    <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">{{question}}</a><button type="button" class="btn btn-default">Edit </button>
					<br>
				                </h4>
				            </div>
				            <div id="collapse" class="panel-collapse collapse in">
				                <div class="panel-body">
				                    <p>{{solution}} 
				                </div>
				            </div>
				        </div>
				       {{/each}}
					</div>
	</script>

	<script type='text/x-handlebars-template' id ='docs-html'>
					<h6> Uber Drivers</h6>
					<button type="button" class="btn btn-default">Add New </button>
					<br>
					<div class="panel-group" id="accordion">
					 	{{#each drivers}}
				        <div class="panel panel-default">
				            <div class="panel-heading">
				                <h4 class="panel-title">
				                    <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">{{name}}</a>
				                </h4>
				            </div>
				            <div id="collapse" class="panel-collapse collapse in">
				                <div class="panel-body">
				                   Lastname : <h6>{{last_name}}</h6><br>
				                   Licence Number :<h6>{{license_number}}</h6><br>
				                   ID Number: <h6>{{id_number}}</h6><br>
				                   Email Address: <h6>{{email_address}}</h6><br>
				                   Vehicle: <h6>{{vehicle}}</h6><br>
				                   City :<h6>{{city}}</h6><br>
				                </div>
				            </div>
				        </div>
				       {{/each}}
					</div>
	</script>

	<script type='text/x-handlebars-template' id ='history-html'>
					<h6> Issue history </h6>
					<div class="panel-group" id="accordion">
				 	{{#each issues}}
				        <div class="panel panel-default">
				            <div class="panel-heading">
				                <h4 class="panel-title">
				                    <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">{{issue_name}}</a>
				                </h4>
				                <h6> Status {{status}} </h6>
				            </div>
				            <div id="collapse" class="panel-collapse collapse in">
				                <div class="panel-body">
				                    <p>{{issue}}</a></p>
				                    <h4> Post a Response </h4>
				                    <input type='textarea' id='response'><button type="button" class="btn btn-default">Post</button>
					<br>
				                </div>
				            </div>
				            
				        </div>
				       {{/each}}
					</div>
	</script>


	<script src='scripts/agenttemplating.js'></script>
	<script>
	   // var user = {{driverData.id}}
		templateDocumentsSection();
		templateFaqsSection();
		templateHistorySection();
	</script
</body>
</html>